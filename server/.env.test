# Test environment configuration
NODE_ENV=test
PORT=8788
API_VERSION=2.0.0

# CORS configuration for tests
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate limiting (more permissive for tests)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_MAX_CONCURRENT=100

# Test API keys (these are fake tokens for testing)
NOTION_TOKEN=secret_test_token_12345678901234567890123456
AIRTABLE_TOKEN=patTEST123456789.1234567890123456789012345678901234567890

# Logging for tests
LOG_LEVEL=error
LOG_FILE=./logs/test.log

# Security
SESSION_SECRET=test-session-secret
HELMET_CSP_ENABLED=false