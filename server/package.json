{
  "name": "chat-session-mgmt-server",
  "version": "2.0.0",
  "description": "Backend API for Chat Session Management Builder",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:auth": "jest tests/auth --verbose",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "winston": "^3.13.0",
    "dotenv": "^16.4.5",
    "@notionhq/client": "^2.2.15",
    "airtable": "^0.12.2",
    "joi": "^17.12.3",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.3",
    "typescript": "^5.4.5",
    "tsx": "^4.9.3",
    "tsc-alias": "^1.8.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "nock": "^13.3.8",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "notion",
    "airtable",
    "chat",
    "session",
    "management",
    "api",
    "typescript",
    "express",
    "authentication",
    "oauth",
    "jwt"
  ],
  "author": "Chat Session Management Team",
  "license": "MIT"
}